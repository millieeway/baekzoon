import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] a = {1, 1, 2, 2, 2, 8}; 
		int[] b = new int [6];
		for(int i = 0; i < b.length; i++) {
			b[i] = sc.nextInt();
		}
		for (int j = 0; j < b.length; j++) {
			if(a[j] == b[j]) {
				System.out.print(0 + " ");
			}else if(a[j] > b[j]) {
				System.out.print(a[j] - b[j] + " ");
			}else if(a[j] < b[j]) {
				System.out.print(a[j] - b[j] + " ");
			}
		}
	}
}
