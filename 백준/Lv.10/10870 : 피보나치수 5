import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		int sum = fibonacci(n);
		System.out.println(sum);
	}
	static int fibonacci(int n) { 
		if(n == 1) return 1;
		if(n == 0) return 0;
		return fibonacci(n-2) + fibonacci(n-1);
	} 
}
